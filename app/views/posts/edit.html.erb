<h2>Edit Post</h2>

<%= form_with(model: @post, url: post_path(@post), method: :patch) do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :content %>
  <%= form.text_area :content %>

  <%= form.submit 'Update Post' %>
<% end %>

<%= link_to 'Back', posts_path %>
